<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/relativeLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#171717"
    android:clipToOutline="true"
    tools:context=".presentation.LandingPageActivity"
    >

    <TextView
        android:id="@+id/te_atreves"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="116dp"
        android:gravity="center_horizontal|center_vertical"
        android:text="@string/te_atreves"
        android:textAppearance="@style/te_atreves_"
        app:layout_constraintTop_toTopOf="parent"
        app:fontFamily = "@font/aileron"/>

    <TextView
        android:id="@+id/udare"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginTop="30dp"
        android:gravity="center_horizontal|top"
        android:text="@string/udare"
        android:textAppearance="@style/udare"
        app:layout_constraintTop_toBottomOf="@id/te_atreves"
        app:fontFamily = "@font/neuemetananext_semibold"/>

    <ImageView
        android:id="@+id/star_1"
        android:layout_width="30dp"
        android:layout_height="30dp"
        android:layout_marginTop="60dp"
        android:src="@drawable/star1"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.64"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/udare"
        />

    <Button
        android:id="@+id/btnregistrarse"
        android:layout_width="232dp"
        android:layout_height="61dp"
        android:layout_marginTop="92dp"
        android:background="@drawable/rectangle_1"
        android:gravity="center_horizontal|center_vertical"
        android:text="Registrarse"
        android:textAppearance="@style/registrarse_reg"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/star_1"
        app:fontFamily = "@font/aileron"
        />

    <Button
        android:id="@+id/btniniciarses"
        android:layout_width="232dp"
        android:layout_height="61dp"
        android:layout_marginTop="30dp"
        android:background="@drawable/rectangle_2"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        android:text="@string/iniciar_ses"
        android:textAppearance="@style/iniciar_ses"
        app:layout_constraintTop_toBottomOf="@+id/btnregistrarse"
        app:fontFamily = "@font/aileron"
        />

</androidx.constraintlayout.widget.ConstraintLayout>

